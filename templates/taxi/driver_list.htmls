{% extends "base.html" %}
{% block content %}
<h1>Driver List</h1>

{% if driver_list %}
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Username</th>
    </tr>
  </thead>
  <tbody>
    {% for driver in driver_list %}
    <tr>
      <td>{{ driver.id }}</td>
      <td><a href="{% url 'taxi:driver-detail' driver.id %}"></a>{{ driver.license_number }}</a></td>
    </tr>
    {% endfor %}
  </tbody>
</table>
{% endif %}
{% endblock %}

